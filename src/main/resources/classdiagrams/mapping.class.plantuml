    + TagSubscriptionManager()
    + addTagToGroup()
    + clear()
    + getDefinition()
    + getGroup()
    + getGroups()
    + getOrCreateDefinition()
    + getSubscriptionGroups()
    + getTagId()
    + getTagIdDefinitionMap()
    + removeTag()
}
interface mapping.TagSubscriptionMapReader {
    {abstract} + getDefinition()
    {abstract} + getGroup()
    {abstract} + getGroups()
    {abstract} + getOrCreateDefinition()
    {abstract} + getTagId()
