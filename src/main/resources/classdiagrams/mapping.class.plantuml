    + SubscriptionGroup()
    + add()
    + contains()
    + getPublishInterval()
    + getTagIds()
    + remove()
    + reset()
    + size()
}
class mapping.TagSubscriptionManager {
    - subscriptionGroups : Map<Integer, SubscriptionGroup>
