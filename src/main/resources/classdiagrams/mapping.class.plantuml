    {static} + of()
    {static} + toNodeId()
    + getClientHandle()
    + getMethodNodeId()
    + getNodeId()
    + getTimeDeadband()
    + getValueDeadband()
    + getValueDeadbandType()
    - ItemDefinition()
}
class mapping.SubscriptionGroup {
