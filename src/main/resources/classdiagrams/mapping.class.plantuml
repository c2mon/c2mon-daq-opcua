}
interface mapping.TagSubscriptionWriter {
    {abstract} + addTagToGroup()
    {abstract} + clear()
    {abstract} + removeTag()
}
mapping.TagSubscriptionManager .up.|> mapping.TagSubscriptionMapReader
mapping.TagSubscriptionManager .up.|> mapping.TagSubscriptionWriter
mapping.TagSubscriptionManager "[publishInterval]" o-left- "*" mapping.SubscriptionGroup
mapping.TagSubscriptionManager "[tagId]" o-- "*" mapping.ItemDefinition
mapping.SubscriptionGroup "[tagId]" o-- "*" mapping.ItemDefinition

mapping.TagSubscriptionWriter -[hidden]left- mapping.TagSubscriptionMapReader